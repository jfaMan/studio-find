<%= simple_form_for [@studio, @booking] do |f| %>
  <%= f.input :start_date, html5: true %>
  <%= f.input :end_date, html5: true  %>
  <%= hidden_field_tag :query, params[:query]  %>
  <% if @booking.id %>
    <%= f.submit 'Update', class: "btn-gradient" %>
  <% else %>
    <%= f.submit 'Book Studio', class: "btn-gradient" %>
  <% end %>
<% end %>
